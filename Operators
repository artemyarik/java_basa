int a = 10;
     int b = 20;
     int c = 25;

a + b = 30 
a - b = -10 
a * b = 200
b / a = 2 
b % a = 0 
c % a = 5 
a++ = 10 
b-- = 11 

a == b = false 
a != b = true 
a > b = false 
a < b = true 
b >= a = true 
b <= a = false




boolean a = true;  
boolean b = false;

a && b = false 
a || b = true 
!(a && b) = true
